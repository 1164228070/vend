/**
 * 获取缓存
 * @param type
 */
cache.get = function(type){
	return cache.data[type];
}

/**
 * 获取缓存明细值
 * @param type
 * @param cacheVal
 * @returns
 */
cache.getName = function(type,cacheVal){
	try {
		return cache.data[type][cacheVal];
	} catch (e) {
		return "--";
	}
}