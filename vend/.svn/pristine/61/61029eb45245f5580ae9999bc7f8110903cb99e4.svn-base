package com.haiyi.domain;
import java.math.BigDecimal;
import java.util.Date;

import com.haiyi.domain.base.BaseDomain;
import com.maizi.anno.ClassInfoAnno;
/**
  * 
  * @ClassName: ComsumeLog
  * @Company: 麦子科技
  * @Description: ComsumeLog实体
  * @author 技术部-刘强峰
